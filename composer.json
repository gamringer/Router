{
    "name": "gamringer/php-json-pointer",
    "description": "A very simple router that routes anything",
    "keywords": ["router", "routing", "route", "dispatch", "dispatcher", "anything", "everything"],
    "homepage": "https://github.com/gamringer/Router",
    "license": "MIT",
    "authors": [
        {
            "name": "Guillaume Amringer",
            "email": "g.amringer@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.6"
    },
    "require-dev": {
        "phpunit/phpunit" : "4.*"
    },
    "autoload": {
        "psr-4": {
            "gamringer\\Router\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "gamringer\\Router\\Test\\": "tests/",
            "gamringer\\Router\\Example\\": "examples/"
        }
    }

}
